
{% spaceless %}
    id="wjb_uploader_{{ form.vars.id }}"
    data-uploader-trigger-button=""
    data-upload-url="{{ path('wjb_uploader_upload', {filter_name: form.vars.filter_name, allowed_mime_types: form.vars.allowed_mime_types}) }}"
    data-prototype="{{ form_widget(form.vars.prototype)|e('html_attr') }}"
    data-target="#wjb_uploader_wrapper_{{ form.vars.id }}"
    data-allowed-mime-types="{{ form.vars.allowed_mime_types|join(',') }}"
{% endspaceless %}